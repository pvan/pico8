	camera(x,y)
	--map()
	
	draw_board()
	
	draw_triggers()
	
	--draw_stickers()
	
	--highlight_stickers(mwx,mwy)
	
	hl_tilehv(tilehv(mwx,mwy))

end


function _draw()

 gtime={}

 quadx={-64,64,-64,64}
 quady={-64,-64,64,64}
 
 if zoomout then
 
	 screen_pixels={}
	 qpix={}
	 for q=1,4 do
	  draw_game(cx+quadx[q],cy+quady[q])		 
	  add(gtime,{"d"..q,stat(1)})
	 
	  qpix[q]=read_screen()
	  add(gtime,{"r"..q,stat(1)})
	 end
	 
	 shkpix={}
	 for q=1,4 do
	  shkpix[q]=shrink_pix(qpix[q])
	  add(gtime,{"s"..q,stat(1)})
	 end
	 
	 cls()
	 
	 for q=1,4 do
	  write_quad(shkpix[q],q)
	 end
	 add(gtime,{"w",stat(1)})
 
 else
  draw_game(cx,cy)
 end
 
 camera()
 cursor()
 spr(64,mx,my)
 print2(stat(1))
 
 
	--timing
	if zoomout then
		lastt = 0
		for i=1,#gtime do
		 delta = gtime[i][2]-lastt
		 --delta=flr(delta*100)
		 print(gtime[i][1].." "..delta)
		 lastt = gtime[i][2]
		end
	end
	

end
