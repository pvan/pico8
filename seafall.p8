function _update()

 msx,msy=get_mouse()
 
 if (btnp(â)) zoomout=not zoomout
 
 if (zoomout) msx/=2 msy/=2
 mwx,mwy=msx+cx,msy+cy
 
 update_triggers()
 
 
 h,v=tilehv(mwx,mwy)
 if h>3 and lmdown() then
  if tilehv_rect(h,v)[3]!=0 then
   place_island(h+1,v+1,h+1)
  end
 end
 
 
