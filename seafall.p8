function place_sticker(s,x,y)
 res={}
 res.x=x
 res.y=y
 res.spr=s.spr
 res.w=s.w
 res.h=s.h
 res.popup=s.popup
 add(stickers,res)
 return res
end




function draw_sticker(s)
 spr(s.spr, s.x,s.y, s.w,s.h)
end
function draw_stickers()
 for s in all(stickers) do
  draw_sticker(s)
 end
end


function highlight_stickers(mx,my)
 for s in all(stickers) do
  if pinrect(mx,my,srect(s)) then
   rect2(srect(s))
   if s.popup!=nil then
    draw_popup(s.popup)
    line(s.x,s.y,64,50)
   end
  end
 end
end

function srect(s)
 return {s.x,s.y,s.w*8,s.h*8}
end




