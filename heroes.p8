-- --could later change this
--end

function tile_is_solid(x,y)
 if x<0 or x>tilesw or
    y<0 or y>tilesh 
 then
  return true
 end
 if fget(mget(x,y),0) then
  return true
 end
 if mapitemtiles[xy2i(x,y)] then
  return true
 end
 return false
end


