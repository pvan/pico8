camx=0
camy=0

mvx=-100
mvy=-100

function _init()
 --music(0)
end

function _update()

 if (btnp(⬅️)) curx-=8
 if (btnp(➡️)) curx+=8
 if (btnp(⬆️)) cury-=8
 if (btnp(⬇️)) cury+=8
 curx=max(curx,0)
 cury=max(cury,0)
 curx=min(curx,worldw)
 cury=min(cury,worldh)
 
 --camera
 camgap = 32
 if curx>camx+64+camgap then camx+=2 end
 if curx<camx+64-camgap then camx-=2 end
 if cury>camy+64+camgap then camy+=2 end
 if cury<camy+64-camgap then camy-=2 end
 camx=max(camx,-worldborder)
 camy=max(camy,-worldborder)
 camx=min(camx,worldw-128+worldborder*2) --minus size of camera
 camy=min(camy,worldh-128+worldborder*2)

	
	--select
 if (btnp(🅾️)) then
  mvx = curx
  mvy = cury
 end

end



function _draw()

	cls()
	
	camera(camx,camy)
	
 --draw_battle_map()
 
 draw_overworld()
 
 --draw path
 spr(161, mvx, mvy)
 
 draw_cursor()

end



curx=64
cury=64
curanim=0
function draw_cursor()

 --rectfill(curx,cury,curx+8,cury+8,15)
 curanim=(curanim+1)%20
 curextra=flr(curanim/10)
 curminus = 4+curextra
 curplus = 4+curextra
 spr(160, curx-curminus, cury-curminus)
 spr(160, curx+curplus, cury-curminus, 1,1, true,false)
 spr(160, curx-curminus, cury+curplus, 1,1, false,true)
 spr(160, curx+curplus, cury+curplus, 1,1, true,true)

end

