--overworld



--size of world
tilesw=32
tilesh=32
worldw=tilesw*8
worldh=tilesh*8

worldborder=8

function draw_overworld()

 cls(3)
 
 --[[
 for x=0,16 do
  for y=0,16 do
   spr(102,x*8,y*8)
  end
 end
 ]]
 
-- for i=0,10 do
 --spr(70, x,y)
 
 
 --border around world
 --(see worldborder)
 for x=0,tilesw do
  spr(97, x*8, -8)
  spr(97, x*8, worldh+8)
 end
 for y=0,tilesh do
  spr(96, -8, y*8)
  spr(96, worldw+8, y*8)
 end
 spr(64, -8,-8)
 spr(65, worldw+8,-8)
 spr(80, -8,worldh+8)
 spr(81, worldw+8,worldh+8)


 
 map(0,0, 0,0 ,16,16)
 
 --91
 --78
 herospr=3
 spr(herospr, 64-4,64-4, 2,2)
 
 castlespr=138
 spr(castlespr, 20,30, 4,4)

end


--[[
--draw sprite with w/h 
--in pixels instead of tiles
--(not super needed, could)
--(just use larger sprite)
--(and leave black space)
function spr2(id, x,y, pw,ph)
 clip(x,y, pw,py)
 spr(id, x,y, ceil(pw/8),ceil(ph/8))
 clip()
end

function subspr2(id,x,y)
 clip(x,y,4,4)
 if id-flr(id)== 0 then spr(id,x  ,y  ) end
 if id-flr(id)==.1 then spr(id,x-4,y  ) end
 if id-flr(id)==.2 then spr(id,x  ,y-4) end
 if id-flr(id)==.3 then spr(id,x-4,y-4) end 
 clip()
end]]
