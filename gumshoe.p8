function palreset()
 pal()
 palt(0,false)
 palt(3,true)
end


function _init()

 init_player()
 palreset()

end



function _update()
 
 
 --debug menu
 if btn(❎) and btn(🅾️) then
  pause_menu=true
  
  --pm=0
  if (btnp(⬆️)) then
   if (pm==3) pm=0 else pm=1
  end
  if (btnp(⬅️)) then
   if (pm==4) pm=0 else pm=2
  end
  if (btnp(⬇️)) then
   if (pm==1) pm=0 else pm=3
  end
  if (btnp(➡️)) then
