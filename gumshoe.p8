--player


--psprs={236,237,238,239}
psprs={128,129,130,131}

function init_player()
	px,py=60,60
	pd=2
	
	pt=0
	
	pclothes=true
	
 hotx={-4,3}
 hoty={-2,2}
	
end

function player_update()

 
 local dx,dy=0,0
 if (btn(⬅️)) dx-=1 
 if (btn(➡️)) dx+=1 
 if (btn(⬆️)) dy-=1 
 if (btn(⬇️)) dy+=1
 
 if dx!=0 or dy!=0 then
	 usebox={px+hotx[1]+dx*4,
	         py+hoty[1]+dy*4,
	         hotx[2]-hotx[1],
	         hoty[2]-hoty[1]}
 end
 if usebox==nil then
	 usebox={px+hotx[1],
	         py+hoty[1],
	         hotx[2]-hotx[1],
	         hoty[2]-hoty[1]}
 end
 
 if pstate=="use" then
	 dx,dy=0,0
  pstatetimeleft-=1
  if pstatetimeleft==0 then
   pstate="stand"
  end
 end
 
 if pstate!="use" then
	 if btnp(❎) and preleaseduse then
	  dx,dy=0,0
	  pstate="use"
	  pstatetimeleft=10
	  palreadused=false
	  preleaseduse=false
	 else
		 if dx!=0 or dy!=0 then
		  pstate="walk"
		 else
		  pstate="stand"
		  pt=1
		 end
	 end
 end
 
 if (not btn(❎)) preleaseduse=true
 
 if (dx>0) pflip=false 
 if (dx<0) pflip=true
 
 
 --collision / movement
 if dx!=0 or dy!=0 then
  local allowx,allowy=true,true 
  for x in all(hotx) do
   for y in all(hoty) do
    local hx,hy=px+x,py+y
    if (solid(hx+dx,hy)) allowx=false
    if (solid(hx,hy+dy)) allowy=false
   end
  end
  if (allowx) px+=dx
  if (allowy) py+=dy
 end
 
 if not palreadused then
	 if pstate=="use" then
		 if usetrig(usebox,rack) then
	   palreadused=true
		 end
	 end
 end
 
 --anim
 pt+=0.2
 if (pt>#psprs) pt=0
 pa=flr(pt)
 
end


function player_draw()
  
 local psprx=px-4
 local pspry=py-15
 
 --do fake project shadow?
 palt(0,false)
 palt(3,true)
 spr(64,psprx,pspry+12)
 palt(0,true)
 palt(3,false)
 
 if pstate=="walk" then
  pspr=psprs[pa+1]
 end
 if pstate=="stand" then
  pspr=psprs[1]
 end
 if pstate=="use" then
  pspr=psprs[4]+1
 end
 if (pclothes) pspr+=(160-128)
 spr(pspr,psprx,pspry,1,2,pflip)

 if debughots then
  pset(px,py,11)
  
  for x in all(hotx) do
  for y in all(hoty) do
  pset(px+x,py+y,10)
  end end
 end
 if debugcols then
  rect2(usebox)
 end

end



