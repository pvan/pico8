  if (pm==3) debugxxxx=not debugxxxx
  if (pm==4) debuguse=not debuguse
  pm=0
 end
 
 
 player_update(dx,dy)


 
end


function special_glass(plr_y)
 
 for obj in all(things) do
 
 --skip doors behind the player
 if obj[2]+obj[4]>plr_y then
 
 --if closed door
 if obj[7]==13 then 
  basex=obj[1]+obj[8]+1
  basey=obj[2]+obj[9]+5

  --red bg
	 for x=0,5 do
	  for y=0,3 do
	   pset(basex+x,basey+y,8)
	  end
	 end
	 
	 --draw over red
	 player_draw()
	 
	 --check what isn't red anymore
	 for x=0,5 do
	  for y=0,3 do
	   col=pget(basex+x,basey+y)
	   if col!=8 then
	    pset(basex+x,basey+y,13)
	   else
	    pset(basex+x,basey+y,6)
	   end
	  end
	 end
	 --draw lettering
	 pset(basex+2,basey+1,0)
	 pset(basex+1,basey+1,0)
	 pset(basex+1,basey+2,0)
	 pset(basex+4,basey+1,0)
	 pset(basex+4,basey+2,0)
	end
	end
	end
end

function _draw()
 cls()
 
 map()
 
 --special_glass()

 ypos={}
 for i=1,#things do
  local obj=things[i]
  local ybase=obj[2]+obj[4]
  add(ypos,{ybase,i})
 end
 add(ypos,{py+hoty[2],0})
 sort_objs(ypos)
 
 --ok this is pretty simple
 --except for the special glass
 --basically draw everything
 --in order of y (sorted above)
 --a "things" index of i=0
 --is code for the player
 --for glass...
 --(pink, only closed doors)
 --if behind player, just 
 --change pink to gray
 --when we draw the player,
 --run our special code to 
 --render the glass on all doors
 --in front of the player
 --and later, when those 
 --door sprites are drawn,
 --change pink to transparent
 --to save the special glass
 --pixels we already rendered
 drewplayer=false
 for i=1,#ypos do
  if ypos[i][2]==0 then
   player_draw()
   special_glass(py+hoty[2])
   drewplayer=true
  else
   local obj=things[ypos[i][2]]
   if obj[7]==13 then
    if drewplayer then  
	    palt(14,true) --transparent
  	 else
 	   pal(14,6) --grey
	   end
   end
   draw_obj(obj)
   palreset()
  end
 end
 
 
