function userack()
-- stop("here")
 pclothes=not pclothes
 rack[6]=34
 if (pclothes) rack[6]=32
end


rack={10,5,7,16, userack,32}




function usetrig(r,trig)
 if rectcol(r,trig)
 then
  trig[5]()
  return true
 end 
 return false
end
