 end
 
 
 --selection icon (fg)
 sely+=4
 for it in all(planes) do
	 if mix==it.x and miy==it.y then
	--  spr(224,selx,sely-8-pa*10,3,2)
	  for i=0,it.a-1 do
	   --224
	   spr(166,selx,sely-8-i*10,2,2)
	  end
	 end
 end
 
 

 -- quilt debug
 --(test tile of every pixel)
 if btn(âŽ) and false then
	 for x=-32,32 do
