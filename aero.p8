	   local tx,ty=screen2iso(x,y)
	   srand(tx+ty*100)
	   pset(x,y,rnd(16))
	  end
	 end
 end
 
 
 camera()
 color(7)
 spr(16,msx,msy)
 dprint(msx..","..msy)
 
 local r=tiles[mix]
 if (r==nil) r={}
