	 if mix==it.x and miy==it.y then
	  pal(12,7)
	  --35
	  spr(130,selx,sely-it.a*10,2,2)
	  sspr(79,71,9,8,selx,sely-it.a*10-3)
	  sspr(72,69,7,11,selx+9,sely-it.a*10-3)
	  pal()
	  --192
	  --spr(164,selx,sely-pa*10-8,2,2)
	 else
