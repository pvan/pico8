 t=0
 
 --player animation
 tick=0
 anim=0
 
 --cycle the map 1 pixel at a time
 pixel_cycle=0 
 
 
 objects={}
 starting_air_pos=128-8-16-2-pixel_cycle
 add_object(0,starting_air_pos,8*11+4)
 
 
 -- other code tab inits
 -- just copy pastes
