function update_balloon(fall)
 baldelay=(baldelay+1)%3
 if baldelay==0 then balanim=(balanim+1)%#balsprs end
 balspr=balsprs[balanim+1]
-- if balanim==0 then balspr=224 end
-- if balanim==1 then balspr=226 end
-- if balanim==2 then balspr=228 end
 
 --how far we travel swaying
 sway=16
 
 if baly<-16 then
  balcol=balcols[rnd_between(1,5)]
  baly=128+rnd_between(0,64)
  maxleft=16+8 --wall plus item
  maxright=128-16-8-16 --wall minus item minus balloon width
  maxleft+=sway/2
  maxright-=sway/2
  --balx=maxright --test limits like this
  balanchorx=rnd_between(maxleft,maxright)
  balt=rnd(1)
 end
 
 --sway
