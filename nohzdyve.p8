--wall colliders
left_col={x=0,y=0,w=8,h=128}
right_col={x=128-8,y=0,w=8,h=128}
 
 
function collide(r1,r2)

 if r1.x+r1.w>r2.x and 
    r1.x<r2.x+r2.w and
    r1.y+r1.h>r2.y and
    r1.y<r2.y+r2.h 
 then
  return true 
 else
  return false
 end

end

function draw_col(col)
 if col!=nil then
  rect(col.x, col.y, col.x+col.w-1, col.y+col.h-1)
 end
end
