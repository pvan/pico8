function restart()
 init_vars()
 sfx(9,3) --window open sound
end


function _init()
 init_vars()
 logo=true
end

function _update()
 
 if restart_timer>0 then restart_timer-=1 end
 if restart_timer==0 then
  restart()
  restart_timer=-1
 end

 animate_air_sprite()
 
 
 for i = 0,6 do
  if btn(i) then
   logo = false
  end
 end
 
 if logo then return end
 
 
 if intro==4 then
  sfx(8,3) --preempt the fall a little
 end
 if intro==0 then
  music(1)
  intro-=1
 end
 if intro>0 then
  sprintx+=3
  py-=1
  intro-=1
  --return
 end
 
 
 fall_speed=2
 if end_game then fall_speed=0 end
 if intro>0 then fall_speed=1 end
 
 
 --main counter for falling
 pixel_cycle=(pixel_cycle+fall_speed)%120
 
 
 -- objects fall
 for obj in all(objects) do
  obj.y-=fall_speed
  if obj.y<-64 then del(objects,obj) end
 end
 
 l1=update_laundry(l1,fall_speed)
 l2=update_laundry(l2,fall_speed)
 
 if intro>0 then return end
 

 update_teeth(fall_speed)
 update_smoke(fall_speed)
 update_balloon(fall_speed)
 
 if end_game then return end
 
 
 --this is quite fragile
 --requires starting intro value to be an odd number
 if pixel_cycle==0 then --new cycle
  air_loc = flr(rnd(8))  --<x
  flr_loc = flr(rnd(8))
  if (flr_loc==air_loc) then
   flr_loc=4 end
  
  if air_loc==0 then
   add_object(0,8+2,       132+8*3) end
  if air_loc==1 then
   add_object(0,128-8-16-2,132+8*3) end
  if air_loc==2 then
   add_object(0,8+2,       132+8*10) end
  if air_loc==3 then
   add_object(0,128-8-16-2,132+8*10) end
     
  if flr_loc==0 then
   add_object(1,8+2,      140+8*3) end
  if flr_loc==1 then
   add_object(1,128-8-8-2,140+8*3) end
  if flr_loc==2 then
   add_object(1,8+2,      140+8*10) end
  if flr_loc==3 then
   add_object(1,128-8-8-2,140+8*10) end
  
 end
 
 
 
 
 -- player pos
 player_shift_speed = 5
 if btn(⬅️) then xspd=-player_shift_speed end
 if btn(➡️) then xspd=player_shift_speed end
 
 px+=xspd
 --xspd/=1.2  --will slow to full stop (nice & easy)
 if xspd>0 then xspd-=2 xspd=max(xspd,2) end
 if xspd<0 then xspd+=2 xspd=min(xspd,-2) end
    
 t+=0.01
 if t>1 then t-=1 end
 ysin = sin(t)*12
 py = 48+ysin
 
 
  
 -- col detection
 plr_col={x=px+3,y=py+3,w=7,h=10}
 
 --balloon col
 bal_col={x=balx+1,y=baly+2,w=11,h=12}
 if collide(plr_col,bal_col) then
  --sfx(6)
  score+=10
  sfx(7,3)
  spawn_smoke(balx,baly)
  baly=-100
 end
 
 --teeth col
 if collide(plr_col,teeth_col()) then
  crash()
 end
 
 --wall col
 if collide(plr_col,left_col)
 or collide(plr_col,right_col)
 then
  crash()
 end
 
 --object col
 cols={}
 for obj in all(objects) do
  if obj.typ==0 then 
   if obj.x>50 then
    obj_col={x=obj.x+2,y=obj.y,w=16,h=16}
   else
    obj_col={x=obj.x-2,y=obj.y,w=16,h=16}
   end
  else 
   obj_col={x=obj.x,y=obj.y,w=8,h=8}
  end
  add(cols,obj_col)
  if collide(obj_col,plr_col) then
   crash()
  end
 end
 

end


end_game=false
function crash()
 music(-1)
 sfx(0,3)
 end_game=true
 spawn_smoke(px,py)
 if score>high then high=score end
 restart_timer=100
end

