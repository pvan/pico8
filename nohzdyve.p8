function update_smoke(fall)
 if not smoke_enabled then 
  smoke_anim=0
  smoke_delay=0
 end
 
 smoke_y -= fall
 
 smoke_delay=(smoke_delay+1)%3
 if (smoke_delay==0) then
  smoke_anim+=1 end
  
 smoke_sprs={12,14,44,46}
 
 local frame_count=#smoke_sprs
 if smoke_anim>=frame_count then
  smoke_enabled = false 
 else
  smoke_sprite=smoke_sprs[smoke_anim+1]
 end
 

