--misc



--x pos to print s in center of screen
--doesn't factor in wide symbols
function hcenter(s)
  -- screen center minus the
  -- string length times the 
  -- pixels in a char's width,
  -- cut in half
  return 64-#s*2
end


--recursive deep copy
--works on non-tables too
function copy(o)
 local c
 if type(o) == 'table' then
  c = {}
  for k, v in pairs(o) do
   c[k] = copy(v)
  end
  else
   c = o
 end
 return c
end

--mirror x value across screen
--optional w for mirroring sprites of width w
--w is same as like mir(x+w/2)-w/2
function mir(x,w)
 if w!=nil then x+=w/2 end
 x-=64
 x*=-1
 x+=64
 if w!=nil then x-=w/2 end
 return x
end


--inclusive (low<=result<=high)
--remove +1 for low<=result<high
function rnd_between(low,high)
 return flr(rnd(high-low+1))+low
end

function test_rnd_between()
 print(rnd_between(2,4))
 print(rnd_between(2,4))
 print(rnd_between(2,4))
 print(rnd_between(2,4))
end
