--laundry

function create_laundry(spawnpos)
 local l={}
 l.pos=spawnpos
 l.typ=flr(rnd(6)) --increase for more blank
 return l
end


function update_laundry(l,fall)
 l.pos-=fall
 if (l.pos < -50) then 
 	l=create_laundry(l.pos+240)
 end
 return l
end

function draw_laundry(l)
 --make sure <=x here equals last possible typ below
 if l.typ<=4 then
  line(15,l.pos,32,l.pos+2,1) 
  line(32,l.pos+2,64,l.pos+3,1)
  line(64,l.pos+3,96,l.pos+2,1)
  line(96,l.pos+2,128-16,l.pos,1)
 end

 if l.typ==0 then
  spr(128, 30,l.pos-2, 2,2)
  spr(130, 75,l.pos, 2,2)
  spr(132, 91,l.pos, 2,2)
 end
 if l.typ==1 then
  spr(132, 25,l.pos, 2,2, true)
  spr(134, 85,l.pos, 2,2)
 end
 if l.typ==2 then
  pal(8,11)
  spr(132, 20,l.pos, 2,2, true)
  pal(10,11)
  spr(134, 61,l.pos+1, 2,2)
  pal(8,12)
  spr(130, 73,l.pos, 2,2)
  spr(136, 87,l.pos, 3,3)
  pal()
 end
 if l.typ==3 then
  spr(160, 38,l.pos-2, 2,2)
  spr(132, 19,l.pos, 2,2, true)
  spr(134, 85,l.pos, 2,2)
 end
 if l.typ==4 then
  pal(8,11)
  spr(132, mir(20,16),l.pos, 2,2)
  pal(10,14)
  spr(134, mir(61,16),l.pos+1, 2,2, true)
  pal(8,14)
  pal(12,14)
  spr(130, mir(73,16),l.pos, 2,2, true)
  spr(136, mir(87,8*3),l.pos, 3,3, true)
  pal()
 end
 --print(l.typ,64,l.pos)
end



