 if (pdir==4) fac=38
 if (pdir==5) fac=32
 if (pdir==6) fac=6
 if (pdir==7) fac=0
 mir=false
 if (pflip) mir=true
 spr(panims[flr(panim)]+fac,pwx-ofx,pwy-ofy,2,2,mir)
